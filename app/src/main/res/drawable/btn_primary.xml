<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

  <item android:state_enabled="false">
    <shape android:shape="rectangle">
      <corners android:radius="2.0dip"/>
      <solid android:color="@color/btn_disable"/>
    </shape>
  </item>

  <item android:state_enabled="true" android:state_focused="false" android:state_pressed="false">
    <shape android:shape="rectangle">
      <corners android:radius="2.0dip"/>
      <solid android:color="@color/primary_btn_normal"/>
    </shape>
  </item>

  <item android:state_enabled="true" android:state_pressed="true">
    <shape android:shape="rectangle">
      <corners android:radius="2.0dip"/>
      <solid android:color="@color/primary_btn_pressed"/>
    </shape>
  </item>

  <item android:state_focused="true">
    <shape android:shape="rectangle">
      <corners android:radius="2.0dip"/>
      <solid android:color="@color/primary_btn_pressed"/>
    </shape>
  </item>


</selector>